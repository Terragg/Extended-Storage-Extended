<Defs>
	<ThingDef>
		<defName>Storage_MassiveWeaponsRack</defName>
		<ExtendedStorageExtended>
			<holdsSummary>heavy weapons</holdsSummary>
		</ExtendedStorageExtended>
	</ThingDef>
