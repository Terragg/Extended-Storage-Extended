<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<ThingDef Name="ExtendedStorageExtended_FurnitureBase" ParentName="ExtendedStorage_FurnitureBase" Abstract="True">
		<thingClass>ExtendedStorage.Building_ExtendedStorage</thingClass>
		<pathCost>60</pathCost>
		<altitudeLayer>Building</altitudeLayer>
		<passability>PassThroughOnly</passability>
		<castEdgeShadows>true</castEdgeShadows>
		<leaveResourcesWhenKilled>True</leaveResourcesWhenKilled>
		<tickerType>Normal</tickerType>
		<staticSunShadowHeight>0.5</staticSunShadowHeight>
		<surfaceType>Item</surfaceType>
		<techLevel>Neolithic</techLevel>
		<thingCategories Inherit="False">
			<li>BuildingsFurnitureStorage</li>
		</thingCategories>
	</ThingDef>

	<ThingDef Name="ExtendedStorageExtended_FurnitureWithQualityBase" ParentName="ExtendedStorageExtended_FurnitureBase" Abstract="True">
		<comps>
			<li>
				<compClass>CompQuality</compClass>
			</li>
		</comps>
	</ThingDef>

	<ThingDef Name="ExtendedStorageExtended_ArtableFurnitureBase" ParentName="ExtendedStorageExtended_FurnitureWithQualityBase" Abstract="True">
		<comps>
			<li Class="CompProperties_Art">
				<nameMaker>NamerArtFurniture</nameMaker>
				<descriptionMaker>ArtDescription_Furniture</descriptionMaker>
				<minQualityForArtistic>Excellent</minQualityForArtistic>
			</li>
		</comps>
		<inspectorTabs>
			<li>ITab_Art</li>
		</inspectorTabs>
	</ThingDef>
</Defs>
